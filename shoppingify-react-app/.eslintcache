} else {\r\n                l.func(appState);\r\n            }\r\n        }\r\n    };\r\n\r\n    useEffect(() => {\r\n        if (listenToSliceUpdate) {\r\n            listeners.push({ componentId: componentId, func: setAppState });\r\n        }\r\n\r\n        return () => {\r\n            if (listenToSliceUpdate) {\r\n                listeners = listeners.filter(l => l.componentId !== componentId);\r\n            }\r\n        };\r\n    }, []);\r\n\r\n    return [appState, dispatch];\r\n};\r\n\r\nexport function initStore(\r\n    reducer: { [actionType: string]: (state: IAppState, action: IAction) => IAppState },\r\n    initialState: IAppState\r\n): void {\r\n    appState = { ...appState, ...initialState };\r\n    appReducer.push(reducer);\r\n};\r\n","D:\\personal\\repos\\shoppingify\\shoppingify-react-app\\src\\services\\utils\\AuthService.ts",[],"D:\\personal\\repos\\shoppingify\\shoppingify-react-app\\src\\components\\auth\\AuthLayout\\AuthLayout.tsx",[],"D:\\personal\\repos\\shoppingify\\shoppingify-react-app\\src\\components\\common\\Layout\\Layout.tsx",[],"D:\\personal\\repos\\shoppingify\\shoppingify-react-app\\src\\components\\History\\History.tsx",[],"D:\\personal\\repos\\shoppingify\\shoppingify-react-app\\src\\components\\auth\\Auth\\Auth.tsx",[],"D:\\personal\\repos\\shoppingify\\shoppingify-react-app\\src\\components\\Statistics\\Statistics.tsx",["191"],"import React from \"react\";\r\nimport css from \"./Statistics.module.css\";\r\n\r\nexport const Statistics = () => {\r\n\r\n    return (\r\n        <div>\r\n            Statistics\r\n        </div>\r\n    );\r\n}","D:\\personal\\repos\\shoppingify\\shoppingify-react-app\\src\\components\\common\\Sidemenu\\Sidemenu.tsx",[],"D:\\personal\\repos\\shoppingify\\shoppingify-react-app\\src\\components\\common\\Sidedrawer\\Sidedrawer.tsx",[],"D:\\personal\\repos\\shoppingify\\shoppingify-react-app\\src\\components\\item\\Items\\Items.tsx",[],"D:\\personal\\repos\\shoppingify\\shoppingify-react-app\\src\\servic